{
  "logic_tree": {
    "name": "客戶詢問邏輯樹",
    "spin_framework": {
      "phases": {
        "S": {
          "name": "Situation",
          "name_zh": "現況了解",
          "purpose": "了解客戶基本情況，建立對話基礎",
          "signals_to_advance": ["客戶已提供公司型態", "客戶已說明業務類型", "客戶已透露現況"]
        },
        "P": {
          "name": "Problem",
          "name_zh": "痛點挖掘",
          "purpose": "發現客戶的困擾、不滿或潛在問題",
          "signals_to_advance": ["客戶承認有困擾", "客戶表達不滿", "客戶提出擔憂"]
        },
        "I": {
          "name": "Implication",
          "name_zh": "影響放大",
          "purpose": "讓客戶意識到問題的嚴重性和後果",
          "signals_to_advance": ["客戶認同問題嚴重", "客戶表達擔心後果", "客戶想要解決"]
        },
        "N": {
          "name": "Need-payoff",
          "name_zh": "解決導向",
          "purpose": "引導客戶看到解決方案的價值，促成行動",
          "signals_to_advance": ["客戶表達興趣", "客戶同意約訪", "客戶詢問下一步"]
        }
      },
      "transition_rules": [
        {
          "from": "S",
          "to": "P",
          "condition": "已了解客戶基本情況（公司型態、業務、現況）",
          "trigger_keywords": ["目前", "現在", "打算", "想要", "計畫"]
        },
        {
          "from": "P",
          "to": "I",
          "condition": "客戶已表達或承認有困擾/問題",
          "trigger_keywords": ["困擾", "麻煩", "問題", "擔心", "不方便", "貴", "壓力"]
        },
        {
          "from": "I",
          "to": "N",
          "condition": "客戶認同問題的嚴重性或影響",
          "trigger_keywords": ["確實", "對", "是啊", "沒錯", "怎麼辦", "怎麼解決"]
        },
        {
          "from": "any",
          "to": "N",
          "condition": "客戶主動表達強烈興趣或急迫性",
          "trigger_keywords": ["想了解", "怎麼開始", "可以約", "什麼時候", "價格多少"]
        }
      ],
      "skip_rules": [
        {
          "scenario": "客戶已明確表達痛點",
          "skip_to": "P",
          "example": "客戶說：「現在租金太貴了」"
        },
        {
          "scenario": "客戶抱怨其他家經驗",
          "skip_to": "I",
          "example": "客戶說：「之前租別家結果被補稅」"
        },
        {
          "scenario": "客戶表達急迫性",
          "skip_to": "N",
          "example": "客戶說：「我下週就要用，怎麼辦」"
        }
      ]
    },
